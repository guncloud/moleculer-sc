{
  "name": "moleculer-sc",
  "version": "0.6.0",
  "description": "API Gateway service for Moleculer framework using SocketCluster",
  "main": "lib/index.js",
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "dev": "babel src -d lib --watch",
    "example": "node examples/simple/server.js",
    "build": "babel src -d lib",
    "test": "ava"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tiaod/moleculer-sc.git"
  },
  "keywords": [
    "socketcluster",
    "moleculer",
    "microservice"
  ],
  "author": "tiaod",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tiaod/moleculer-sc/issues"
  },
  "homepage": "https://github.com/tiaod/moleculer-sc#readme",
  "dependencies": {
    "debug": "^3.1.0",
    "lodash": "^4.17.10",
    "moleculer": "^0.12.4",
    "nanomatch": "^1.2.9",
    "socketcluster-client": "^13.0.0"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "p-event": "^1.3.0"
  },
  "ava": {
    "files": [
      "test/**/*.js"
    ],
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  }
}
